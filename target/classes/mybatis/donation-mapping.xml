<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="Donation">
    <!-- 상품 목록 -->
    <select id="donationList" resultType="DonationDTO">
        <![CDATA[
        select fundingNum,
               fundingName,
               fundingMaxPrice,
               fundingDes,
               fundingImg,
               gdsDate
        from Funding_Projects
        order by fundingNum desc
        ]]>
    </select>

</mapper>